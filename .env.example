# LiveKit Cloud credentials
LIVEKIT_URL=wss://your-project.livekit.cloud
LIVEKIT_PUBLIC_URL=wss://your-project.livekit.cloud
LIVEKIT_API_KEY=your_api_key
LIVEKIT_API_SECRET=your_api_secret

# LiveKit Meet URL (for test calls via ./call script)
LIVEKIT_MEET_URL=https://meet.livekit.io

# Agent name (must match dispatch rule)
LIVEKIT_AGENT_NAME=test_agent

# xAI Grok Voice API (required for speech-to-speech)
XAI_API_KEY=your_xai_api_key

# Optional - Timezone for agent (IANA format)
AGENT_TIMEZONE=UTC

# Optional - Web search
EXA_API_KEY=

# Optional - S3 Recording
ENABLE_RECORDING=false
S3_BUCKET=
S3_REGION=
ACCESS_SUPABASE=
SECRET_SUPABASE=
ENDPOINT_SUPABASE=
